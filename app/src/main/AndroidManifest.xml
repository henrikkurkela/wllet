<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wllet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Wllet">
        <activity android:name=".SendRequestActivity"></activity>
        <activity android:name=".PrivacyPolicyActivity"></activity>
        <activity android:name=".TermsAndConditionsActivity"></activity>
        <activity android:name=".DealsActivity"></activity>
        <activity android:name=".ExchangeActivity" />
        <activity android:name=".AddCardActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".WalletActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>